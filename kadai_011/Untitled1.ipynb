      "source": [
        "# リストを設定\n",
        "planets = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "# for文でリストの内容を順番に出力\n",
        "for planet in planets:\n",
        "  print(planet)\n",
        "\n",
        "# 変数iを0からスタート\n",
        "i = 0\n",
        "# while文でリストの個数を確認し、その個数の回数出力\n",
        "while i < len(planets):\n",
        "  print(planets[i])\n",
        "  # カウントを1ずつ増やす\n",
        "  i = i + 1"
      ]
