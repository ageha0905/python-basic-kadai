      "source": [
        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "  # 名前と年齢情報を出力する関数\n",
        "  def printinfo(self):\n",
        "    print(f\"名前：{self.name}\")\n",
        "    print(f\"年齢：{self.age}歳\")\n",
        "# Humanクラスのインスタンスを作る\n",
        "human = Human (\"侍太郎\", 36)\n",
        "# 関数を呼び出す\n",
        "human.printinfo()"
      ]
    }
  ]
